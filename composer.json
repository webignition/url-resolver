{
    "name": "webignition/url-resolver",
    "description": "Resolve a given URL to the URL it actually points to; follow 30X and meta redirects to achieve this",
    "keywords": ["url", "resolver"],
    "homepage": "https://github.com/webignition/url-resolver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\Url\\Resolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\Url\\Resolver\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle":"~6",
        "webignition/web-page-model":">=3.1.1,<4",
        "webignition/absolute-url-deriver":">=2,<3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "minimum-stability":"stable"
}