{
    "name": "webignition/url-resolver",
    "description": "Resolve a given URL to the URL it actually points to; follow 30X and meta redirects to achieve this",
    "keywords": ["url", "resolver"],
    "homepage": "https://github.com/webignition/url-resolver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\Url\\Resolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\Url\\Resolver\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle":">=5.2.0,<5.3",
        "webignition/web-page-model":">=1.0.6,<2",
        "webignition/url":">=1.9.8,<2",
        "webignition/absolute-url-deriver":">=1.4.1.2,<2",
        "webignition/cookie-url-matcher":">=0.1,<1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "minimum-stability":"stable"
}